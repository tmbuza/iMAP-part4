@article{Bolyen2019,
abstract = {For almost two decades, individuals in the United States have enjoyed a broad right of access to their medical records and other protected health information under the Privacy Rule of the Health Insurance Portability and Accountability Act (HIPAA)1 . The exercise of this right is increasingly instrumental to efforts to promote patient engagement and data sharing. However, although it is clear that much clinical data is subject to the HIPAA access right, questions linger regarding its applicability to personal data generated in clinical research},
